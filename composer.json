{
    "name": "stevepop/chuck-norris-jokes",
    "description": "Create random Chuck Norris Jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Popoola",
            "email": "stevepop@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Stevepop\\ChuckNorrisJokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stevepop\\ChuckNorrisJokes\\Test\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stevepop\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "Stevepop\\ChuckNorrisJokes\\Facades\\CheckNorris"
            }
        }
    }
}
